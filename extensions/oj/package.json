{
  "name": "oj",
  "displayName": "oj",
  "description": "oj",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.73.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "oj.helloWorld",
        "title": "Hello World"
      }
    ],
    "configuration": [
      {
        "title": "oj-config",
        "properties": {
          "oj-config.project.url": {
            "type": "string",
            "description": "题目gitee仓库地址",
            "scope": "workspace"
          },
          "oj-config.project.token": {
            "type": "string",
            "description": "用户Token",
            "scope": "workspace"
          },
          "oj-config.project.id": {
            "type": "number",
            "description": "题目id",
            "scope": "workspace"
          },
          "oj-config.oj.renew.pod.interval": {
            "type": "number",
            "default": 180,
            "description": "pod续约周期(秒)",
            "scope": "workspace"
          },
          "oj-config.oj.countdown.interval": {
            "type": "number",
            "default": 60,
            "description": "倒计时提示周期(秒)",
            "scope": "workspace"
          },
          "oj-config.oj.countdown.threshold": {
            "type": "number",
            "default": 180,
            "description": "倒计时提示周期(秒)",
            "scope": "workspace"
          },
          "oj-config.oj.countdown.check.idle.interval": {
            "type": "number",
            "default": 2,
            "description": "检查挂机行为周期(秒)",
            "scope": "workspace"
          },
          "oj-config.oj.countdown.idle.threshold": {
            "type": "number",
            "default": 60,
            "description": "挂机认定阈值",
            "scope": "workspace"
          }
        }
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.73.1",
    "@vscode/test-electron": "^2.2.2",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "axios": "^1.3.2"
  }
}